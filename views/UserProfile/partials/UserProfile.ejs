
        <p class="page-label">My Profile</p>
        <div class="profile-wrapper">

            <div class="c1">
                <img src="/profile.svg" alt="profile">
                <button>Select Image</button>
                <p>
                    File Size: Maximum 1 MB <br>
                    File Extension: .JPEG, .PNG
                </p>
            </div>

            <div class="c2">
                <!-- TOP SECTION -->
                <div class="c2-1">

                    <div class="input-wrapper">
                        <label for="l-name">Last Name</label>
                        <input type="text" id="l-name">
                    </div>

                    <div class="input-wrapper">
                        <label for="uname">Username</label>
                        <input type="text" id="uname">
                    </div>

                </div>

                <!-- BOTTOM SECTION -->
                <div class="c2-2">
                    <div class="input-wrapper">
                        <label for="address">Complete Address</label>
                        <input type="text" id="address">
                    </div>

                    <div class="input-wrapper">
                        <label for="phone">Phone Number</label>
                        <input type="tel" id="phone">
                    </div>

                    <div class="input-wrapper">
                        <label for="email">Email</label>
                        <input type="email" id="email">
                    </div>
                </div>

            </div>

            <div class="c3">
                <span>Gender</span>
                <div class="radio-wrapper">

                    <div class="radio-group">
                        <label for="male">Male</label>
                        <input type="radio" name="gender" id="male" value="male">
                    </div>

                    
                    <div class="radio-group">
                        <label for="female">Female</label>
                        <input type="radio" name="gender" id="Female">
                    </div>

                    
                    <div class="radio-group">
                        <label for="pns">Other</label>
                        <input type="radio" name="gender" id="pns">
                    </div>
                </div>

                <div class="input-wrapper">
                    <p for="date-of-birth" class="c3-lbl">Date of Birth</p>
                    <input type="date" id="date-of-birth">
                </div>

                <div class="input-wrapper">

                    <p for="country" class="c3-lbl">Country</p>
                    <input type="text" id="country">
                </div>
            </div>

        </div>

        <button class="edit-btn">Edit</button>

    